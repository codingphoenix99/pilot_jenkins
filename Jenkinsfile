# This is Jenkins Declarative Pipeline 
# It has 04 Main Components Pipeline , Agent , Stages , Steps in stages 

pipeline {
    agent any {

    }
    stages{
        stage('One'){
            steps{
                echo 'this is stage 01'
            }
        }
        stage('second'){
            steps {
                echo 'this is stage 02'
            }
        }
        stage ('third'){
            steps {
                echo 'this is stage 03'
            }
        }

    }

}